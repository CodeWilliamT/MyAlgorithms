<?xml version="1.0" encoding="utf-8"?>
<UiConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Window>
    <Text>景焱直线AOI控制与检测软件</Text>
    <WindowState>Maximized</WindowState>
    <BorderStyle>Fixed3D</BorderStyle>
  </Window>
  <Tabs>
    <Level1_Property>
      <Width>100</Width>
      <Height>60</Height>
      <Alignment>Bottom</Alignment>
      <Appearance>Normal</Appearance>
    </Level1_Property>
    <Level1_List>
      <Level1>
        <Text>流程(Jobs)</Text>
        <Level2_Property>
          <Width>180</Width>
          <Height>40</Height>
          <Alignment>Top</Alignment>
          <Appearance>Normal</Appearance>
        </Level2_Property>
        <Level2_List>
          <Level2>
            <Text>工作界面</Text>
            <Assembly>LeadframeAOI-III</Assembly>
            <NameSpace>LeadframeAOI</NameSpace>
            <Class>FormJobs</Class>
          </Level2>
        </Level2_List>
      </Level1>
      <Level1>
        <Text>机构(Devices)</Text>
        <Level2_Property>
          <Width>180</Width>
          <Height>40</Height>
          <Alignment>Top</Alignment>
          <Appearance>Normal</Appearance>
        </Level2_Property>
        <Level2_List>
          <Level2>
            <Text>上料机构参数</Text>
            <Assembly>LeadframeAOI-III</Assembly>
            <NameSpace>LeadframeAOI</NameSpace>
            <Class>frmLoadModules</Class>
          </Level2>
          <Level2>
            <Text>下料机构参数</Text>
            <Assembly>LeadframeAOI-III</Assembly>
            <NameSpace>LeadframeAOI</NameSpace>
            <Class>frmUnloadModules</Class>
          </Level2>
          <Level2>
            <Text>平台机构参数</Text>
            <Assembly>LeadframeAOI-III</Assembly>
            <NameSpace>LeadframeAOI</NameSpace>
            <Class>frmChuckModules</Class>
          </Level2>
          <Level2>
            <Text>相机参数</Text>
            <Assembly>LeadframeAOI-III</Assembly>
            <NameSpace>LeadframeAOI</NameSpace>
            <Class>FormExternalDevices</Class>
          </Level2>
          <Level2>
            <Text>系统标定</Text>
            <Assembly>LeadframeAOI-III</Assembly>
            <NameSpace>LeadframeAOI</NameSpace>
            <Class>FormUV2XY</Class>
          </Level2>
          <!--<Level2>
            <Text>扫描测试</Text>
            <Assembly>LeadframeAOI-III</Assembly>
            <NameSpace>LeadframeAOI</NameSpace>
            <Class>FrmScanTest</Class>
          </Level2>-->
        </Level2_List>
      </Level1>
      <Level1>
        <Text>产品(Products)</Text>
        <Level2_Property>
          <Width>180</Width>
          <Height>40</Height>
          <Alignment>Top</Alignment>
          <Appearance>Normal</Appearance>
        </Level2_Property>
        <Level2_List>
          <Level2>
            <Text>产品、料盒参数</Text>
            <Assembly>LeadframeAOI-III</Assembly>
            <NameSpace>LeadframeAOI</NameSpace>
            <Class>frmProduct</Class>
          </Level2>
          <Level2>
            <Text>采图测试</Text>
            <Assembly>LeadframeAOI-III</Assembly>
            <NameSpace>LeadframeAOI</NameSpace>
            <Class>frmCaptureImage</Class>
          </Level2>
          <Level2>
            <Text>生成产品图谱</Text>
            <Assembly>LeadframeAOI-III</Assembly>
            <NameSpace>LeadframeAOI</NameSpace>
            <Class>FrmAutoMapping</Class>
          </Level2>
          <Level2>
            <Text>产品模板创建</Text>
            <Assembly>LFAOIModel</Assembly>
            <NameSpace>LFAOIModel</NameSpace>
            <Class>ModelCreateUI</Class>
          </Level2>
          <Level2>
            <Text>产品检测参数</Text>
            <Assembly>LeadframeAOI-III</Assembly>
            <NameSpace>LeadframeAOI</NameSpace>
            <Class>FrmInspectPara</Class>
          </Level2>
          <Level2>
            <Text>离线检测</Text>
            <Assembly>LeadframeAOI-III</Assembly>
            <NameSpace>LeadframeAOI</NameSpace>
            <Class>FunctionTest</Class>
          </Level2>
          <Level2>
            <Text>产品复判</Text>
            <Assembly>LFAOIReview</Assembly>
            <NameSpace>LFAOIReview</NameSpace>
            <Class>Form_Review</Class>
          </Level2>
        </Level2_List>
      </Level1>
      <Level1>
        <Text>日志(Log)</Text>
        <Level2_Property>
          <Width>180</Width>
          <Height>40</Height>
          <Alignment>Top</Alignment>
          <Appearance>Normal</Appearance>
        </Level2_Property>
        <Level2_List>
          <Level2>
            <Text>文件</Text>
            <Assembly>LeadframeAOI-III</Assembly>
            <NameSpace>LeadframeAOI</NameSpace>
            <Class>frmLog</Class>
          </Level2>
        </Level2_List>
      </Level1>
      <Level1>
        <Text>系统(System)</Text>
        <Level2_Property>
          <Width>180</Width>
          <Height>40</Height>
          <Alignment>Top</Alignment>
          <Appearance>Normal</Appearance>
        </Level2_Property>
        <Level2_List>
          <Level2>
            <Text>系统设置</Text>
            <Assembly>LeadframeAOI-III</Assembly>
            <NameSpace>LeadframeAOI</NameSpace>
            <Class>frmSystemConfig</Class>
          </Level2>
        </Level2_List>
      </Level1>
      <Level1>
        <Text>警告(Alarms)</Text>
      </Level1>
      <Level1>
        <Text>帮助(Help)</Text>
        <Level2_Property>
          <Width>180</Width>
          <Height>40</Height>
          <Alignment>Top</Alignment>
          <Appearance>Normal</Appearance>
        </Level2_Property>
        <Level2_List>
          <Level2>
            <Text>版本日志</Text>
            <Assembly>LeadframeAOI-III</Assembly>
            <NameSpace>LeadframeAOI</NameSpace>
            <Class>Form_Lv_Inf</Class>
          </Level2>
        </Level2_List>
      </Level1>
    </Level1_List>
  </Tabs>
