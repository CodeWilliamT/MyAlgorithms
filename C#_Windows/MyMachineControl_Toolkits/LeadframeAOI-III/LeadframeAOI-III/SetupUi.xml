  <User>
    <!--<DataFile>HTUser.db</DataFile> 也可以在程序中SetupUi()函数之后配置-->
    <Roles>
      <string>Engineer</string>
      <string>Technician</string>
      <string>Operater</string>
    </Roles>
    <ExtendInfo>
      <string>Email</string>
      <string>Phone</string>
    </ExtendInfo>
  </User>
</UiConfiguration>
